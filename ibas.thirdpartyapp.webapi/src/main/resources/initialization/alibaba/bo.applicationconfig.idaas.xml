<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE ApplicationConfig>
<ns:ApplicationConfig xmlns:ns="https://colorcoding.org/ibas/thirdpartyapp/bo">
	<Code>IDAAS</Code>
	<Name>阿里身份认证服务（基于OIDC协议）</Name>
	<Group>SSO</Group>
	<ApplicationConfigItems>
		<ApplicationConfigItem>
			<Name>client_id</Name>
			<Description>应用标记</Description>
			<Category>TEXT</Category>
		</ApplicationConfigItem>
		<ApplicationConfigItem>
			<Name>client_secret</Name>
			<Description>应用密钥</Description>
			<Category>PASSWORD</Category>
		</ApplicationConfigItem>
		<ApplicationConfigItem>
			<Name>scope</Name>
			<Description>获取用户信息内容</Description>
			<Value>openid email profile</Value>
			<Category>TEXT</Category>
		</ApplicationConfigItem>
		<ApplicationConfigItem>
			<Name>response_type</Name>
			<Description>授权模式</Description>
			<Value>code</Value>
			<Category>TEXT</Category>
		</ApplicationConfigItem>
		<ApplicationConfigItem>
			<Name>redirect_uri</Name>
			<Description>登录后重定向地址</Description>
			<Value></Value>
			<Category>TEXT</Category>
		</ApplicationConfigItem>
		<ApplicationConfigItem>
			<Name>authorize_endpoint</Name>
			<Description>授权端点</Description>
			<Category>TEXT</Category>
		</ApplicationConfigItem>
		<ApplicationConfigItem>
			<Name>token_endpoint</Name>
			<Description>令牌端点</Description>
			<Category>TEXT</Category>
		</ApplicationConfigItem>
		<ApplicationConfigItem>
			<Name>userinfo_endpoint</Name>
			<Description>用户信息端点</Description>
			<Category>TEXT</Category>
		</ApplicationConfigItem>
	</ApplicationConfigItems>
</ns:ApplicationConfig>